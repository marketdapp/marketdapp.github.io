{
  "compilerOptions": {
    "target": "es2020",
    "module": "esnext",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": false,
    "skipLibCheck": true,
    "jsx": "react-jsx",
    "baseUrl": "./src",
    "rootDir": "./src",
    "moduleResolution": "bundler",
    "types": ["vite/client"],
  },
  "include": ["src/**/*"],
}
